<?xml version="1.0" encoding="utf-8"?>
<Project>
    <PropertyGroup>
		<!-- Runtime identity (stable) -->
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<!-- Windows + diagnostics -->
		<FileVersion>$(PackageVersion)</FileVersion>
		<InformationalVersion>$(PackageVersion)</InformationalVersion>
		
		<Company>qwebsolutions.eu</Company>
		<Owners>qwebsolutions.eu</Owners>
		<Authors>qwebsolutions.eu</Authors>
        <Copyright>Copyright Â© $(Company) $([System.DateTime]::Now.Year)</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://metapsi.dev</PackageProjectUrl>
    </PropertyGroup>
</Project>